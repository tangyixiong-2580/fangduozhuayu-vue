.det-root {
    width: 100%;
    height: 100%;

    .header {
        position: relative;
        display: flex;
        align-items: center;
        flex-direction: column;
        color: rgb(255, 255, 255);
        padding: 30px 15px 28px;
        overflow: hidden;

        .header-bg {
            background-image: url("https://img.duozhuayu.com/970b996879d011e78cef00163e063441.jpeg?x-oss-process=image/resize,w_450/quality,Q_80");
            position: absolute;

            top: -50px;
            z-index: -1;
            filter: blur(25px);
            background: center center / cover rgb(214, 186, 140);
            background-position: center center;
        }

        .header-bg::before {
            content: "";
            display: block;
            width: 100%;
            height: 100%;
            background: rgba(0, 0, 0, 0.4);
        }

        .book-cover {
            width: 150px;
            height: auto;

            .cover-img {
                box-shadow: rgba(0, 0, 0, 0.2) 0px 1px 10px;
                display: block;
                max-width: 100%;
                max-height: 100%;
            }
        }

        .bg-set {
            transform: scale(0.3);
            z-index: -1;
            transform-origin: 50% 5%;
            position: fixed;
            top: 0px;
            left: 0px;
            width: 100vw;
            height: 100vh;
            display: flex;
            align-items: center;
            overflow: hidden;

            .cover-img {
                box-shadow: rgba(0, 0, 0, 0.2) 0px 1px 10px;
                display: block;
                max-width: 100%;
                max-height: 100%;
            }
        }

    }

    .all-context {
        padding: 18px 0px;
        background: rgb(255, 255, 255);
        margin-bottom: 65px;

        .user-rec {
            display: flex;
            align-items: center;
            margin-bottom: 15px;
            padding: 0px 15px;
            background: rgb(255, 255, 255);

            .rec-modal {
                display: flex;
                align-items: center;

                .user-header {
                    display: inline-block;
                    vertical-align: top;
                    box-sizing: border-box;
                    width: 30px;
                    height: 30px;
                    background: rgb(242, 242, 242);
                    border-width: 1px;
                    border-style: solid;
                    border-color: rgb(242, 242, 242);
                    border-image: initial;
                    border-radius: 99px;
                    overflow: hidden;

                    .user-header-set {
                        display: block;
                        width: 100%;
                    }


                }

                .user-name {
                    display: inline-block;
                    vertical-align: top;
                    max-width: 6em;
                    overflow: hidden;
                    white-space: nowrap;
                    text-overflow: ellipsis;
                    padding-right: .25em;

                }

                .Icon {
                    width: 1.4em;
                    vertical-align: -.3em;
                    margin-right: -1%;
                }
            }
        }

        .desc-content {
            padding-top: 15px;
            border-top: 0.5px solid rgb(242, 242, 242);
            /* padding: 0px 0px 15px; */
            margin: 0px 15px;
            background: rgb(255, 255, 255);
            margin-bottom: 15px;

            .bookname-set {
                font-size: 20px;
                font-weight: 500;
                text-align: left;
                color: rgb(51, 51, 51);
                user-select: initial;
                margin: -5px 0px 0px;
            }

            .content-price {
                display: flex;
                flex-direction: row;
                -webkit-box-align: center;
                align-items: center;
                margin-top: 17px;

                .price-set {
                    line-height: 1;
                    font-size: 24px;
                    font-weight: 500;
                }

                .price-discount {
                    line-height: 0;
                    margin: 0px 6px;

                    .discount-set {
                        height: 22px;
                        color: rgb(242, 129, 129);
                        border-color: rgb(242, 129, 129);
                        /* color: rgb(214, 186, 140); */
                        border: .5px solid rgb(242, 129, 129);
                        box-sizing: border-box;
                        display: inline-flex;
                        align-items: center;
                        /* height: 18px; */
                        padding: 0 6px;
                        font-size: 12px;
                        font-weight: 400;
                        border-radius: 4px;
                        white-space: nowrap;
                    }
                }
            }

            .content-more {
                margin-top: 14px;

                .condition {
                    display: flex;
                    flex-direction: row;
                    line-height: 1.69;
                    font-size: 13px;

                    .condition-set {
                        flex-shrink: 0;
                        width: 39px;
                        position: relative;
                        color: rgb(170, 170, 170);
                        font-weight: 300;
                        text-align: justify;
                        font-size: 0px;

                        .font-set {
                            font-size: 13px;
                        }

                        .font-set::before {
                            content: ":";
                            margin-left: 2px;
                            position: absolute;
                            top: -2px;
                            right: -5px;
                            line-height: 1.69;
                            color: rgb(170, 170, 170);
                            font-size: 13px;
                        }

                        .font-set::after {
                            content: "";
                            display: inline-block;
                        }
                    }

                    .condition-type {
                        display: flex;
                        flex-direction: row;
                        -webkit-box-align: center;
                        align-items: center;
                        margin-left: 15px;
                        position: relative;

                        .type-set {
                            line-height: 1.31;
                            font-size: 13px;
                            color: rgb(102, 102, 102);
                        }

                        .type-link {
                            margin-left: 6px;
                            line-height: 0;
                            font-size: 9px;

                            .Icon {
                                width: 1.4em;
                                vertical-align: -.3em;
                                margin-right: -1%;
                            }
                        }
                    }
                }

                .original-price {
                    display: flex;
                    flex-direction: row;
                    line-height: 1.69;
                    font-size: 13px;

                    .condition-set {
                        flex-shrink: 0;
                        width: 39px;
                        position: relative;
                        color: rgb(170, 170, 170);
                        font-weight: 300;
                        text-align: justify;
                        font-size: 0px;

                        .font-set {
                            font-size: 13px;
                        }

                        .font-set::before {
                            content: ":";
                            margin-left: 2px;
                            position: absolute;
                            top: -2px;
                            right: -5px;
                            line-height: 1.69;
                            color: rgb(170, 170, 170);
                            font-size: 13px;
                        }

                        .font-set::after {
                            content: "";
                            /* width: 100%; */
                            display: inline-block;
                        }
                    }

                    .condition-type {
                        display: flex;
                        flex-direction: row;
                        -webkit-box-align: center;
                        align-items: center;
                        margin-left: 15px;
                        position: relative;

                        .type-set {
                            line-height: 1.31;
                            font-size: 13px;
                            color: rgb(102, 102, 102);
                        }
                    }
                }

            }
        }

        .douban {
            padding-top: 15px;
            border-top: 0.5px solid rgb(242, 242, 242);
            /* padding: 0px 0px 15px; */
            margin: 0px 15px;
            padding-bottom: 15px;

            .douban-set {
                align-items: center;
                flex-direction: row;
                display: flex;

                .douban-desc {
                    flex-grow: 1;
                    margin-left: 10px;
                    color: rgb(214, 186, 140);
                    font-size: 13px;
                    text-align: left;

                    .desc-score {
                        margin-left: 5px;
                        font-size: 15px;
                    }
                }

                .desc-star {
                    position: absolute;
                    width: 13px;
                }
            }

            .Icon {
                width: 1.4em;
                vertical-align: -.3em;
                margin-right: -1%;
            }
        }

        .self {
            padding-top: 15px;
            border-top: 0.5px solid rgb(242, 242, 242);
            margin: 0px 15px;
            background: rgb(255, 255, 255);
            padding-bottom: 15px;

            .douban-set {
                align-items: center;
                flex-direction: row;
                display: flex;

                .self-desc {
                    flex-grow: 1;
                    margin-left: 10px;
                    color: rgb(214, 186, 140);
                    font-size: 13px;
                    text-align: left;
                    text-decoration: none;
                }

                .Icon {
                    width: 1.4em;
                    vertical-align: -.3em;
                    margin-right: -1%;
                }
            }
        }

        .introduce {
            padding-top: 15px;
            border-top: 0.5px solid rgb(242, 242, 242);
            margin: 0px 15px;
            background: rgb(255, 255, 255);

            .introduce-header {
                margin: 0px;
                padding-bottom: 35px;
                padding: 0px 0px 15px;
                background: rgb(255, 255, 255);

                .header-set {
                    display: flex;
                    flex-direction: row;
                    align-items: center;
                    padding-top: 15px;
                    padding-right: 3px;

                    .fishFriend {
                        font-size: 18px;
                        font-weight: normal;
                        line-height: 1.56;
                        text-align: left;
                    }
                }

                .comment {
                    display: flex;
                    align-items: center;
                    cursor: pointer;
                    // .comment{
                    margin-left: 5px;
                    font-size: 15px;
                    font-weight: normal;
                    color: rgb(24, 195, 170);
                    // }
                }
            }

            .intro {
                padding-bottom: 20px;
                padding-left: 0px;
                padding-right: 0px;
                margin-bottom: 15px;
                /* padding: 0px 15px; */
                background: rgb(255, 255, 255);

                .intro-header {
                    font-size: 18px;
                    font-weight: normal;
                    line-height: 1.56;
                    text-align: left;
                }

                .intro-content {
                    line-height: 1.8;
                    white-space: pre-wrap;
                    overflow-wrap: break-word;
                }

                .findMore {
                    display: flex;
                    justify-content: center;
                    margin-top: 12px;

                    .tofind {
                        color: rgb(24, 195, 170);
                        font-size: 13px;
                        padding: 5px 1em;

                        .Icon {
                            width: 1.4em;
                            vertical-align: -.3em;
                            margin-right: -1%;
                        }
                    }
                }
            }

            .correlation {
                padding-left: 0px;
                padding-right: 0px;
                margin-bottom: 15px;
                /* padding: 0px 15px; */
                background: rgb(255, 255, 255);

                .conrrelation-header {
                    font-size: 18px;
                    font-weight: normal;
                    line-height: 1.56;
                    text-align: left;
                }

                .correlation-title {
                    display: inline-block;
                    vertical-align: top;
                    margin: 5px;

                    .title-set {
                        display: inline-flex;
                        align-items: center;
                        box-sizing: border-box;
                        height: 30px;
                        padding: 0px 9px;
                        border-width: 1px;
                        border-style: solid;
                        border-color: rgb(230, 230, 230);
                        border-image: initial;
                        border-radius: 5px;
                    }
                }

                .correlation-list {
                    display: flex;
                    flex-direction: column;
                    flex: 1 1 0%;

                    .list-cover {
                        display: inline-block;
                        vertical-align: top;
                        width: calc((100% - 60px) / 3);
                        margin: 10px;

                        .cover-set {
                            position: relative;
                            font-size: 13px;
                            text-align: center;
                            overflow-wrap: break-word;

                            .cover-content {
                                box-sizing: border-box;
                                box-shadow: rgba(0, 0, 0, 0.2) 1px 1px 8px;
                                width: auto;
                                height: calc(((100vw - 80px) / 3) * 1.42);
                                position: relative;
                                display: flex;
                                align-items: center;
                                justify-content: center;
                                flex-shrink: 0;

                                /* width: 110px;
                                height: 156px; */
                                .cover-img {
                                    display: block;
                                    max-width: 100%;
                                    max-height: 100%;
                                    width: 100%;
                                    height: 100%;
                                    background-size: cover;
                                    background-position: center center;
                                    background-repeat: no-repeat;
                                }
                            }

                            .cor-bookname {
                                margin-top: 6px;
                                white-space: nowrap;
                                text-overflow: ellipsis;
                                overflow: hidden;
                                font-size: 13px;
                                text-align: center;
                                overflow-wrap: break-word;
                            }
                        }
                    }
                }
            }
        }

        .fixed {
            position: fixed;
            bottom: 0;
            right: 0;
            left: 0;
            z-index: 9999;

            .fixed-content {
                display: flex;
                height: 64px;
                flex-direction: row;
                align-items: center;
                background-color: rgb(255, 255, 255);
                border-top: 0.5px solid rgb(230, 230, 230);

                .home-link {
                    line-height: 0;
                    margin: 0px 27px;
                }

                .fixed-set {
                    width: 1px;
                    height: 44px;
                    background-color: rgb(242, 242, 242);
                }

                .cart-link {
                    position: relative;
                    line-height: 0;
                    margin: 0px 30px;

                    .cart-count {
                        position: absolute;
                        top: -3px;
                        left: 50%;
                        margin-left: 5px;
                        box-sizing: border-box;
                        height: 18px;
                        width: 18px;
                        color: #fff;
                        font-size: 12px;
                        font-weight: 400;
                        line-height: 18px;
                        text-align: center;
                        background: rgb(242, 129, 129);
                        border-radius: 9px;
                    }
                }
            }

            .addCart {
                flex-grow: 1;
                margin-right: 15px;

                .addCart-set {
                    border-color: rgb(242, 129, 129);
                    padding: 0 1.2em;
                    border-radius: 999px;
                    height: 44px;
                    font-size: 15px;
                    font-weight: 500;
                    color: #fff;
                    background-color: rgb(242, 129, 129);
                    text-align: center;

                    .adds {
                        display: flex;
                        justify-content: center;
                        align-items: center;
                        height: 100%;
                    }
                }
            }
        }
    }

    .fixed {
        position: fixed;
        bottom: 0px;
        left: 0px;
        height: 65px;
        width: 100%;
        background-color: #fff;
        border-top: 0.5px solid rgb(230, 230, 230);

        .fixed-content {
            display: flex;
            flex-direction: row;
            height: 100%;

            .home {
                display: flex;
                justify-content: center;
                align-items: center;
                width: 20%;
                height: 100%;
                border-right: solid 0.5px rgb(242, 242, 242);
            }

            .cart {
                display: flex;
                justify-content: center;
                align-items: center;
                width: 25%;
                height: 100%;
                position: relative;

                .cart-count {
                    position: absolute;
                    top: 15px;
                    left: 50%;
                    margin-left: 5px;
                    height: 18px;
                    width: 18px;
                    color: #fff;
                    font-size: 12px;
                    font-weight: 400;
                    line-height: 18px;
                    text-align: center;
                    background: rgb(242, 129, 129);
                    border-radius: 9px;
                }
            }

            .intoCart {
                display: flex;
                justify-content: center;
                align-items: center;
                height: 100%;
                width: 55%;

                .cart-unexistence {
                    background-color: rgb(242, 129, 129);
                    padding: 0 3.8em;
                    border-radius: 999px;
                    height: 44px;
                    font-size: 15px;
                    font-weight: 500;
                    color: #fff;
                    display: flex;
                    justify-content: center;
                    align-items: center;
                    margin-right: 15px;
                }

                .cart-existence {
                    background-color: rgba(230, 230, 230, 0.5);
                    padding: 0 3.2em;
                    border-radius: 999px;
                    height: 44px;
                    font-size: 15px;
                    font-weight: 500;
                    color: rgb(170, 170, 170);
                    display: flex;
                    justify-content: center;
                    align-items: center;
                    margin-right: 15px;
                }

                .appearances {
                    background-color: rgb(242, 129, 129);
                    padding: 0 3.2em;
                    border-radius: 999px;
                    height: 44px;
                    font-size: 15px;
                    font-weight: 500;
                    color: #fff;
                    display: flex;
                    justify-content: center;
                    align-items: center;
                    margin-right: 15px;
                }

                .appearances-choose.van-popup.van-popup--bottom {
                    padding: 20px;
                    border-radius: 10px 10px 0px 0px;
                    width: 90%;

                    .title {
                        display: flex;
                        flex-direction: column;

                        .title-name {
                            font-size: 18px;
                            font-weight: 500;
                            color: rgb(51, 51, 51);
                        }

                        .appearances-rule {
                            margin-top: 2px;
                            line-height: 1.46;
                            font-size: 13px;
                            color: rgb(170, 170, 170);
                            display: flex;
                            align-items: center;

                            .appearances-showRules {
                                display: flex;
                                align-items: center;
                                margin-left: 5px;
                                font-size: 13px;
                                line-height: 1.46;
                                color: rgb(51, 51, 51)
                            }
                        }
                    }

                    .appearances-list {
                        margin-top: 16px;
                        margin-bottom: 12px;

                        .appearances-content {
                            display: flex;
                            box-sizing: border-box;
                            height: 72px;
                            margin-bottom: 12px;
                            flex-direction: row;
                            align-items: center;
                            padding: 0px 13px 0px 18px;
                            border-radius: 4px;
                            border-width: 1px;
                            border-style: solid;
                            border-color: rgb(242, 242, 242);

                            &.select {
                                border-color: rgb(242, 129, 129);
                                border-width: 1px;
                            }

                            .price {
                                font-weight: 500;
                                line-height: 1.2;
                                font-size: 22px;
                                margin-right: 10px;
                            }

                            .appearances-text {
                                color: rgb(102, 102, 102);
                                line-height: 1.31;
                                font-size: 13px;

                                &.new {
                                    color: rgb(214, 186, 140);
                                }
                            }
                        }
                    }

                    .choose {
                        background: rgb(242, 129, 129);
                        color: #fff;
                        height: 40px;
                        border-radius: 3px;
                        padding: 0 1em;
                        display: flex;
                        justify-content: center;
                        align-items: center;
                    }

                }

                .cart-existence-appearances {
                    background-color: rgba(230, 230, 230, 0.5);
                    padding: 0 3.2em;
                    border-radius: 999px;
                    height: 44px;
                    font-size: 15px;
                    font-weight: 500;
                    color: rgb(102, 102, 102);
                    display: flex;
                    justify-content: center;
                    align-items: center;
                    margin-right: 15px;
                }

                .stock-unexistence {
                    background-color: rgb(24, 195, 170);
                    padding: 0px 4.3em;
                    border-radius: 999px;
                    height: 44px;
                    font-size: 15px;
                    font-weight: 500;
                    color: #fff;
                    display: flex;
                    justify-content: center;
                    align-items: center;
                    margin-right: 15px;
                }
            }

        }

    }

}